        "eslint-plugin-prettier": "^2.7.0",
        "jquery": "^3.4",
        "laravel-mix": "^2.0",
        "lodash": "^4.17.21",
        "material-design-icons-iconfont": "^3.0.3",
        "popper.js": "^1.12",
        "prettier": "^1.14.3",
        "prettier-eslint": "^8.8.2",
        "prettier-eslint-cli": "^4.7.1",
        "vue": "^2.5.7",
        "vue-router": "^3.0.1",
